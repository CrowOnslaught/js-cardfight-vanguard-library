{
  "name": "ts-cardfight-vanguard-api",
  "version": "1.0.4",
  "description": "A typescript based library that fetches data (using axios) from card-fight-vanguard-api created by Brent Pappas. Look at his documentation for more information.",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "rollup -c",
    "prettier": "prettier --write .",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges"
  },
  "keywords": [
    "cardfight",
    "vanguard",
    "api",
    "cards",
    "cardfight! vanguard",
    "javascript",
    "typescript",
    "Brent Pappas"
  ],
  "author": "crow-onslaught",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CrowOnslaught/js-cardfight-vanguard-library.git"
  },
  "badges": [],
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "babel-jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-node-dev": "^1.1.8"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "coverageReporters": [
      "json-summary", 
      "text",
      "lcov"
    ]
  }
}
